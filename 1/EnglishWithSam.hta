<html xmlns:v="urn:schemas-microsoft-com:vml"  xmlns:o="urn:schemas-microsoft-com:office:office">
<head>
<hta:application 
id="oHTA" 
applicationName="CourseLab"
Icon="icon.ico"
singleInstance="yes"
maximizeButton="no"
minimizeButton="yes"
sysMenu="yes"
scroll="no"
contextmenu="no"
selection="no"
border="thin"
innerBorder="no"
navigable="no"/>
<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7" />
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title></title>
<style>
v\:* {behavior:url(#default#VML);}
o\:* {behavior:url(#default#VML);}
</style>
<link href="../courseimages/courselab.css" rel="stylesheet" type="text/css"/>
<!--[if lt IE 7]><link rel="stylesheet" type="text/css" href="../courseimages/ie6.css" /><![endif]-->
<link href="images/course.css" rel="stylesheet" type="text/css"/>
<script type="text/javascript" src="../courseimages/courselab.js"></script>
<script type="text/javascript" src="../courseimages/lms.js"></script>
<script type="text/javascript" src="../courseimages/legacy.js"></script>
<script type="text/javascript" src="module.js"></script>
</head>
<body style="margin: 0;" onload="Run(false)" onunload="Shutdown()">
<script language=JavaScript>
var nRequiredWidth = 792;
var nRequiredHeight = 536;

var nWidth = nRequiredWidth;
var nHeight = nRequiredHeight;
var nLeft = screen.width / 2 - nWidth / 2;
var nTop = screen.height / 2 - nHeight / 2;

// Resize window
window.resizeTo(nRequiredWidth, nRequiredHeight);
//window.moveTo(nLeft, nTop);

// Get body size
var rc = document.body.getBoundingClientRect();
var nBodyWidth = rc.right - rc.left;
var nBodyHeight = rc.bottom - rc.top;

// Calculate window size and position
nWidth = nRequiredWidth + (nRequiredWidth - nBodyWidth);
nHeight = nRequiredHeight + (nRequiredHeight - nBodyHeight);
nLeft = screen.width / 2 - nWidth / 2;
nTop = screen.height / 2 - nHeight / 2;

// Resize and position window
window.resizeTo(nWidth, nHeight);	
window.moveTo(nLeft, nTop);
</script>
<div id="boardFrame" style="position:absolute;left:0px;top:0px;width:792;height:536"></div>
</body>
</html>
