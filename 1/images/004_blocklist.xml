				<param type="button" id="CANCEL" text="Cancel" x="140" y="34" w="50" h="14"/>
			</params>
      </param>
		<param type="label" text="Inner margin:" x="10" y="126" w="100" h="13"/>
		<param type="edit" id="blocklist_margin" text="5" def="5" x="110" y="125" w="18" h="13" />
		<param type="label" text="pixels" x="130" y="126" w="68" h="13"/>
    </page>
</params>
