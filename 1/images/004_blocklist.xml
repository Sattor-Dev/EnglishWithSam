<params>
	<object>
		<label>Standard Block-List</label>
		<width>350</width>
		<height>52</height>
		<description>Standard Block-List</description>
	</object>
	<page name=" Parameters " w="210" h="148">
      <param type="label" text="Border color:" x="10" y="10" w="100" h="13"/>
