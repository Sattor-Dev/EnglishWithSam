<params>
	<object>
		<label>External URL</label>
		<width>22</width>
		<height>20</height>
		<description>External URL to open in new browser window</description>
	</object>
	<page name="Parameters" w="240" h="220">
		<param type="label" text="" x="11" y="-50" w="6" h="13"/>
		<param type="check" id="standard" text="Use one of default icons" def="yes" x="10" y="10" w="220" h="13">
			<events>
				<event name="check" target="lab_n1_img" action="disable"/>
				<event name="check" target="n1_img" action="disable"/>
				<event name="uncheck" target="lab_n1_img" action="enable"/>
				<event name="uncheck" target="n1_img" action="enable"/>
			</events>
		</param>
		<param type="line" x="10" y="24" w="220" h="26"/>
		<param type="label" id="lab_n1_img" text="Icon:" x="15" y="31" w="100" h="13"/>
		<param type="file" id="n1_img" text="" def="" x="110" y="30" w="93" h="13"/>
		<param type="label" text="URL:" x="10" y="58" w="100" h="13"/>
		<param type="edit" id="jump_url" text="" def="http://" x="110" y="57" w="120" h="13"/>
		<param type="label" text="Window size" x="10" y="75" w="85" h="13"/>
		<param type="label" text="X:" x="95" y="75" w="15" h="13"/>
		<param type="edit" id="wx" text="" def="800" x="110" y="74" w="25" h="13"/>
		<param type="label" text="px" x="138" y="75" w="60" h="13"/>
		<param type="label" text="Y:" x="95" y="92" w="15" h="13"/>
		<param type="edit" id="wy" text="" def="600" x="110" y="91" w="25" h="13"/>
		<param type="label" text="px" x="138" y="92" w="60" h="13"/>
		<param type="check" id="wtoolbar" text="Enable window toolbar" def="no" x="10" y="105" w="220" h="13"/>
		<param type="check" id="wlocation" text="Enable window address bar" def="no" x="10" y="122" w="220" h="13"/>
		<param type="check" id="wstatus" text="Enable window status bar" def="no" x="10" y="139" w="220" h="13"/>
		<param type="check" id="wmenubar" text="Enable window menu bar" def="no" x="10" y="156" w="220" h="13"/>
		<param type="check" id="wresize" text="Enable window resize" def="no" x="10" y="173" w="220" h="13"/>
		<param type="check" id="wscroll" text="Enable window scrollbars" def="no" x="10" y="190" w="220" h="13"/>
	</page>
</params>
